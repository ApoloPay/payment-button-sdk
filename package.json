{
  "name": "payment-button-sdk",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo dev",
    "dev:react": "turbo dev --filter=playground-react",
    "dev:vue": "turbo dev --filter=playground-vue",
    "dev:svelte": "turbo dev --filter=playground-svelte",
    "dev:angular": "turbo dev --filter=playground-angular",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "turbo": "2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0"
}
