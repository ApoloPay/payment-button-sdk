{
  "name": "@payment-button-sdk/react",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --clean --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --dts --watch --external react"
  },
  "dependencies": {
    "@payment-button-sdk/core": "workspace:*"
  },
  "devDependencies": {
    "react": "^18.0.0",
